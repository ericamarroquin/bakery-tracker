@{
  Layout = "_Layout";
}
@using BakeryTracker.Models;

<h4>All current orders for @Model["vendor"].VendorName</h4>

<ul>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'></a></li>
  }
</ul>

<button>
  <a href='/vendors/@Model["vendor"].Id/orders/new'>Add another order</a>
</button>
<button>
  <a href="/vendors">Return to all vendors</a>
</button>
<button>
  <a href="/">Return to Home Page</a>
</button>